    <ExtIngressPeregrineMS>false</ExtIngressPeregrineMS>
    <IntIngressPeregrineMS>false</IntIngressPeregrineMS>
    <TLSPeregrineMS>false</TLSPeregrineMS>
    <LEPeregrineMS>false</LEPeregrineMS>
    <LBPeregrineMS>false</LBPeregrineMS>
    <LBPeregrineMSExternal>false</LBPeregrineMSExternal>
    <ExtIngressElastic>false</ExtIngressElastic>
    <IntIngressElastic>false</IntIngressElastic>
    <TLSElastic>false</TLSElastic>
    <LEElastic>false</LEElastic>
    <LBElastic>false</LBElastic>
    <LBElasticExternal>false</LBElasticExternal>
  </AksSettings>
