    <LogFolderCleanupSchedule>10</LogFolderCleanupSchedule>
    <SavedHistoryCleanupSchedule>0</SavedHistoryCleanupSchedule>
    <NeuronHybridManagementSuiteHost>localhost</NeuronHybridManagementSuiteHost>
    <NeuronServiceWCFTracingEnabled>false</NeuronServiceWCFTracingEnabled>
    <TempDirectory />
