    <TcpPublishingEndPort>50100</TcpPublishingEndPort>
    <AllocatedMemory>0</AllocatedMemory>
    <AllocatedCores>0</AllocatedCores>
    <DockerHosts />
    <EnablePortSharing>false</EnablePortSharing>
    <Administrators />
    <Users />
    <UseGmsa>false</UseGmsa>
    <GmsaAccount>LocalService</GmsaAccount>
    <SwarmDeployment>false</SwarmDeployment>
    <K8SDeployment>true</K8SDeployment>
  </DockerSettings>
  <DockerEnvironmentConfig>
    <esbInstanceName>Default</esbInstanceName>
    <esbEnvironment />
    <esbZone>Enterprise</esbZone>
    <esbDeploymentGroup />
